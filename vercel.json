{
  "version": 2,
  "builds": [
    { "src": "dist/server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/dist/server.js" }
  ],
  "functions": {
    "dist/server.js": { "runtime": "nodejs18.x" }
  },
  "installCommand": "pnpm install",
  "buildCommand": "pnpm build"
} 